{
  "permissions": {
    "allow": [
      "WebSearch",
      "Bash(compgen -c)",
      "Bash(pip install:*)",
      "Bash(python -m pytest:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(railway logs:*)",
      "Bash(railway whoami:*)",
      "Bash(railway version:*)",
      "Bash(railway environment:*)",
      "Bash(railway up:*)",
      "Bash(claude mcp:*)",
      "Bash(railway --version:*)",
      "Bash(railway help:*)",
      "Bash(git push:*)",
      "WebFetch(domain:docs.railway.com)",
      "Bash(railway:*)",
      "Bash(curl:*)",
      "Bash(python:*)",
      "Bash(cmd.exe /c \"railway:*)",
      "Bash(cmd.exe /c \"railway deployment list\")",
      "Bash(cmd.exe /c \"railway logs --deployment ac774e9c-73e3-4686-95d0-ad251e51c02f\")",
      "Bash(cmd.exe /c \"railway logs --build --deployment ac774e9c-73e3-4686-95d0-ad251e51c02f\")",
      "Bash(cmd.exe /c \"railway logs\")",
      "Bash(cmd.exe /c \"railway logs --lines 50 --since 2m\")",
      "Bash(cmd.exe /c \"railway logs --deployment 1f399759-aec1-4368-a160-f54dc22db50e\")",
      "Bash(cmd.exe /c \"railway link --project smiths-li-fastapi --service speed_to_lead\")",
      "Bash(cmd.exe /c \"railway logs --build --lines 50 cd32bd9a-7614-4b31-b76f-d16adb419fa8\")",
      "Bash(cmd.exe /c \"railway logs --lines 100\")",
      "Bash(cmd.exe /c \"railway logs --build --lines 60 230d8d96-a079-4122-8125-26fa7878041a\")",
      "Bash(cmd.exe /c \"railway logs --lines 30\")",
      "Bash(cmd.exe /c \"railway logs --lines 40\")",
      "Bash(cmd.exe /c \"railway logs --lines 15\")",
      "Bash(cmd.exe /c \"railway logs --lines 20\")",
      "Bash(cmd.exe /c \"railway logs --lines 10\")",
      "Bash(cmd.exe /c \"railway logs --lines 50\")",
      "WebFetch(domain:karozieminski.substack.com)",
      "WebFetch(domain:www.anthropic.com)",
      "Bash(pytest:*)",
      "Bash(cmd.exe /c \"railway logs --build --lines 50 f0980c37-9b1f-4191-b5be-f18eb6271451\")",
      "Bash(chmod:*)"
    ]
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "./scripts/pre-commit-check.sh",
            "timeout": 120000
          }
        ]
      }
    ]
  }
}
